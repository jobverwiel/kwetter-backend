{
  "AllowedHosts": "*",
  "RabbitMQPublisher": {
    "Host": "rabbitmq",
    "Username": "guest",
    "Password": "guest",
    "Exchange": "User"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://seq.logging.svc.cluster.local" }
      }
    ],
    "Properties": {
      "Application": "UserService"
    }
  }
}  
